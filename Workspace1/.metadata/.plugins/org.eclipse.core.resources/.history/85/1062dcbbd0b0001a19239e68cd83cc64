package parameterization;
import utilities.*;
import org.testng.annotations.DataProvider;
import org.testng.annotations.Test;

public class TestNGParameterization {
	public static ExcelReader excel = null;
	@Test(dataProvider = "getData")
	public void doLogin(String Username, String Password, String is_Correct) {
		System.out.println(Username+" "+" "+" "+is_Correct);
	}
	
	@DataProvider
	public Object[][] getData(){
	Object [][] data = null;	
		if (excel==null) {
			excel = new ExcelReader(".//src//test//resources//Excel//TestNG.xlsx");
		}
	String sheetName ="Test1";	
	int rowNum = excel.getRowCount(sheetName);
	int colNum = excel.getColumnCount(sheetName);
	
	for(int row=2;row<=rowNum;row++) {
		for(int col=0; col<=colNum; col++) {
			data = excel.getCellData(sheetName, col, row);
		}
	}
	return data;
		
	}

}
